{
  "name": "hex-problems-exporter",
  "version": "1.2.0",
  "description": "VS Code extension to export and analyze project diagnostics",
  "engines": {
    "vscode": "^1.60.0",
    "node": ">=16.0.0"
  },
  "main": "./dist/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts --max-warnings 0",
    "lint:fix": "eslint src --ext ts --fix",
    "test": "node ./out/test/runTest.js",
    "pretest": "npm run clean && npm run lint && npm run compile",
    "test:unit": "jest --config jest.config.js",
    "test:integration": "node ./out/test/runTest.js",
    "clean": "rimraf out/",
    "watch": "tsc -watch -p ./"
  },
  "dependencies": {
    "exceljs": "^4.3.0",
    "papaparse": "^5.4.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^8.0.1",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/papaparse": "^5.3.7",
    "@types/sinon": "^10.0.0",
    "@types/sinon-chai": "^4.0.0",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vscode/test-electron": "^2.3.0",
    "chai": "^5.0.0",
    "chai-as-promised": "^8.0.1",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "jest": "^29.5.0",
    "mocha": "^10.2.0",
    "sinon": "^15.0.2",
    "sinon-chai": "^4.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.5"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:hexProblemsExporter.export"
  ],
  "contributes": {
    "commands": [
      {
        "command": "hexProblemsExporter.export",
        "title": "Hex Problems Exporter: Export Problems"
      }
    ]
  }
}
