{
  "name": "hex-problems-exporter",
  "version": "1.2.0",
  "description": "VS Code extension to export and analyze project diagnostics",
  "type": "module",
  "main": "dist/extension.js",
  "engines": {
    "vscode": "^1.60.0",
    "node": ">=16.0.0"
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "lint": "eslint src --ext ts"
  },
  "dependencies": {
    "@vscode/vscode-languageserver-textdocument": "^1.0.8",
    "vscode-languageserver-types": "^3.17.3",
    "exceljs": "^4.4.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/chai": "^4.3.0",
    "@types/jest": "^29.0.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^18.0.0",
    "@types/sinon": "^10.0.0",
    "@types/sinon-chai": "^3.2.0",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "chai": "^4.3.0",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.0",
    "expect": "^29.0.0",
    "jest": "^29.0.0",
    "jest-extended": "^4.0.0",
    "mocha": "^9.1.0",
    "rollup": "^3.0.0",
    "sinon": "^16.1.1",
    "sinon-chai": "^3.7.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "categories": ["Other"],
  "activationEvents": ["onCommand:hexProblemsExporter.export"],
  "contributes": {
    "commands": [
      {
        "command": "hexProblemsExporter.export",
        "title": "Hex Problems Exporter: Export Problems"
      }
    ]
  }
}
